<h2>取引先新規登録</h2>

<%=form_with model:@customer,url: customers_path(@customer.id),local: true do |f|%>
  <%=f.label :取引先名 %><br>
   <%= f.text_field :customer_name %><br>

  <%=f.label :郵便番号 %><br>
   <%= f.text_field :zip %><br>

  <%=f.label :都道府県名 %><br>
   <%= f.collection_select(:prefecture_id,Prefecture.all, :id, :name,{})  %><br>

  <%=f.label :市町村名 %><br>
   <%= f.text_field :city_block %><br>

  <%=f.label :ビル名等 %><br>
   <%= f.text_field :build %><br>

  <%=f.label :電話番号 %><br>
   <%= f.text_field :telephone %><br>

  <%=f.label :ファックス番号 %><br>
   <%= f.text_field :facsimile %><br>
 
  <%= f.submit "登録する" %>
<% end %>
